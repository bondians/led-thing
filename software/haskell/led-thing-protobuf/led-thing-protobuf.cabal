                        Proto.LedThing.DeleteChannelRequest,
                        Proto.LedThing.ChannelDriver,
                        Proto.LedThing.WriteChannelResponse,
                        Proto.LedThing.CreateChannelResponse,
                        Proto.LedThing.ChannelFormat,
                        Proto.LedThing.ChannelStructure,
                        Proto.LedThing.ResultStatus,
                        Proto.LedThing.ListChannelsResponse,
                        Proto.LedThing.DeleteChannelResponse,
                        Proto.LedThing.HelloRequest,
                        Proto.LedThing.ProtocolVersion,
                        Proto.LedThing.WireMessage,
                        Proto.LedThing.WireMessage.Message,
                        Proto.LedThing.ListChannelsRequest

  build-depends:        base >= 3,
                        protocol-buffers,
                        protocol-buffers-descriptor
